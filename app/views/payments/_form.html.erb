<%= simple_form_for([@store, @payment]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :rank %>
    <%= f.input :bei %>
    <%= f.input :image %>
    <%= f.input :account %>
    <%= f.association :store %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
