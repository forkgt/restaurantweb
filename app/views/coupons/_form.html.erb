<%= simple_form_for(@coupon) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :desc %>
    <%= f.input :rank %>
    <%= f.input :avatar %>
    <%= f.input :price %>
    <%= f.input :minimum %>
    <%= f.input :start_at %>
    <%= f.input :end_at %>
    <%= f.association :store %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
